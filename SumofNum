
package JavaPlacement;
import java.util.Scanner;

public class Sum {
public static void main(String[]args)
{
	int n,sum = 0;
	System.out.println("Enter the number");
			Scanner src = new Scanner(System.in);
			n = src.nextInt();
			for(int i=1;i<=n;i++)
			{
				 sum = sum+i;
			}
			System.out.println(sum);
			
}
}
